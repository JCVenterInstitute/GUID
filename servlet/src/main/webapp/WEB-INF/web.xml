<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
   "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
   <display-name>Guid Servlet</display-name>
   <description>Supports GUID access via servlet</description>
   <servlet>
      <servlet-name>GuidClientServer</servlet-name>
      <servlet-class>org.jtc.app_db_interface.guid.web.GuidServlet</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>GuidClientServer</servlet-name>
      <url-pattern>/</url-pattern>
   </servlet-mapping>
</web-app>
